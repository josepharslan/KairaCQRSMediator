@model CreateCategoryCommand

@{
    ViewData["Title"] = "CreateCategory";
    Layout = "~/Areas/Admin/Views/Layout/Index.cshtml";
}

<h3>Kategori Ekle</h3>
<form method="post">
    <label>Kategori Adı</label>
    <input type="text" asp-for="CategoryName" class="form-control" />
    <br />
    <label>Kategori Görseli</label>
    <input type="text" asp-for="ImageUrl" class="form-control" />
    <br />
    <button class="btn btn-primary">Ekle</button>
</form>


