@model List<GetServiceQueryResult>
@{
    int count = 0;
}
<section class="features py-5">
    <div class="container">
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-md-3 text-center" data-aos="fade-in" data-aos-delay="@count">
                    <div class="py-5">
                        <svg width="38" height="38" viewBox="0 0 24 24">
                            <use xlink:href="#@item.Icon"></use>
                        </svg>
                        <h4 class="element-title text-capitalize my-3">@item.Title</h4>
                        <p>@item.Description</p>
                    </div>
                </div>
                count += 300;
            }
        </div>
    </div>
</section>
